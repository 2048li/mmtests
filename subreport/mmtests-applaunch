echo
echo MMTests Statistics: application launch

for APPLICATION in `cat app-launch-$KERNEL_BASE-* | awk '{print $1}' | sort | uniq`; do
	printf "%-20s" "$APPLICATION"
	for DIRNAME in $KERNEL_BASE $KERNEL_COMPARE; do
		if [ ! -e app-launch-$DIRNAME-$FTRACE_TEST ]; then
			continue
		fi
		AVG_TIME=`grep "^$APPLICATION" app-launch-$DIRNAME-$FTRACE_TEST | awk '{print $2}' | mean`
		printf "%8.2f" $AVG_TIME
	done
	echo
done
echo
