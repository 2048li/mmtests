echo
echo SYSBENCH
gendirlist sysbench
printheader
CLIENTS=`awk '{print $1}' sysbench-$KERNEL_BASE/$TOPLEVEL/default/sysbench.log`

for DIR in $DIRLIST; do
	INPUTS="$INPUTS `pwd`/$DIR/$TOPLEVEL/default/sysbench-graph.log"
	TITLES="$TITLES `echo $DIR | sed -e 's/sysbench-//'`"
	echo -n > `pwd`/$DIR/$TOPLEVEL/default/sysbench-graph.log
done

for CLIENT in $CLIENTS; do
	# Cope with stupid log entries from an obsolete version of mm tests
	if [ ! -e sysbench-$KERNEL_BASE/$TOPLEVEL/default/sysbench-raw-$CLIENT-1 ]; then
		continue
	fi

	printf "%12d" $CLIENT
	VANILLA=`grep "^$CLIENT " sysbench-$KERNEL_BASE/$TOPLEVEL/default/sysbench.log | awk '{print $2}'`
	for DIR in $DIRLIST; do
		RESULT=`grep "^$CLIENT " $DIR/$TOPLEVEL/default/sysbench.log | awk '{print $2}'`
		CONFIDENCE_LIMIT=2
		while [ "$RESULT" = "" ]; do
			CONFIDENCE_LIMIT=$((CONFIDENCE_LIMIT+1))
			RESULT=`awk '{print $2}' $DIR/$TOPLEVEL/default/sysbench-$CLIENT.log | check-confidence.pl --limit $CONFIDENCE_LIMIT --print-mean`
		done

		if [ "$VANILLA" = "" ]; then
			VANILLA=$RESULT
		fi
		echo $CLIENT $RESULT >> $DIR/$TOPLEVEL/default/sysbench-graph.log
		printf " %8.2f (%5.2f%%)" $RESULT `pdiff $RESULT $VANILLA`
	done
	echo
	printf "%12s" "+/-"
	for DIR in $DIRLIST; do
		CONFIDENCE_LIMIT=1
		STDDEV=""
		while [ "$STDDEV" = "" ]; do
			CONFIDENCE_LIMIT=$((CONFIDENCE_LIMIT+1))
			RESULT=`awk '{print $2}' $DIR/$TOPLEVEL/default/sysbench-$CLIENT.log | check-confidence.pl --limit $CONFIDENCE_LIMIT --print-mean`
			STDDEV=`awk '{print $2}' $DIR/$TOPLEVEL/default/sysbench-$CLIENT.log | check-confidence.pl --limit $CONFIDENCE_LIMIT --print-stddev`
		done
			
		printf " %8.2f%%" `perl -e "print ($STDDEV*100/$RESULT)"`
	done
	echo
done
